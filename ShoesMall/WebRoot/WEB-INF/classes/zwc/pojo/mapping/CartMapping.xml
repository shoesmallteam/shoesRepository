<?xml version="1.0" encoding="UTF-8"?>

<class name="zwc.pojo.Cart" table="cart">
    <property name="cartid" column="cartid" type="String"></property>
    <property name="goods" column="goods" type="String"></property>
    <property name="accountid" column="accountid" type="String"></property>

    <insert id="insertCart">
        insert into cart(cartid,goods,accountid) 
        values(#{cartid},#{goods},#{accountid});
    </insert>
    
    <select id = "selectAll" returnType = "zwc.pojo.Cart">
    	select * from cart
    </select>
	
	
	
</class>